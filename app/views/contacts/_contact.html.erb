<div id="<%= dom_id contact %>">
  <p>
    <strong>Firstname:</strong>
    <%= contact.firstname %>
  </p>

  <p>
    <strong>Lastname:</strong>
    <%= contact.lastname %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= contact.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= contact.phone %>
  </p>

  <p>
    <strong>Cellphone:</strong>
    <%= contact.cellphone %>
  </p>

</div>
